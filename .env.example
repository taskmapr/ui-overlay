# TaskMapr Configuration
# Copy this file to .env and fill in your values

# Agent Endpoint
# URL where your AI agent backend is hosted
# Examples:
#   Local development: http://localhost:8000/api/agent
#   Production: https://your-agent-api.com/agent
VITE_AGENT_ENDPOINT=http://localhost:8000/api/agent

# OpenAI API Key (if using OpenAI models)
# Get your key from: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=sk-your-api-key-here

# Optional: Custom API Token for your agent backend
# VITE_AGENT_API_TOKEN=your-custom-token

# Optional: Agent Framework
# Options: openai-agents, swarm, custom
# VITE_AGENT_FRAMEWORK=openai-agents

# Optional: AI Model
# VITE_AGENT_MODEL=gpt-4o
